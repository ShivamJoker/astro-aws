{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": [
				"src/**/*",
				"package.json",
				"tsconfig.json",
				"tsconfig.build.json",
				"*.config.*",
				"../yarn.lock",
				"../../yarn.lock"
			],
			"outputs": ["dist/**/*"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": [],
			"inputs": [
				"src/**/*",
				"test/**/*",
				"package.json",
				"tsconfig.json",
				"tsconfig.build.json",
				"*.config.*",
				"../yarn.lock",
				"../../yarn.lock"
			]
		},
		"lint": {
			"dependsOn": ["^build"],
			"inputs": ["**/*"],
			"outputs": []
		},
		"synth": {
			"dependsOn": ["build"],
			"outputs": ["cdk.out/**/*"]
		},
		"deploy": {
			"dependsOn": ["synth"],
			"inputs": ["cdk.out/**/*", "dist/**/*"],
			"outputs": []
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"lint:fix": {
			"dependsOn": ["^build"],
			"inputs": ["**/*"],
			"outputs": []
		}
	}
}

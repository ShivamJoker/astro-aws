{
	"name": "@astro-aws/adapter",
	"version": "0.0.4",
	"keywords": [
		"astro-adapter",
		"aws",
		"aws-lambda"
	],
	"homepage": "https://astro-aws.lshay.dev/",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/lukeshay/astro-aws.git",
		"directory": "packages/astro-aws-adapter"
	},
	"license": "MIT",
	"type": "module",

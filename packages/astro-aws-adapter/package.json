	"devDependencies": {
		"@astro-aws/scripts": "workspace:^",
		"@types/aws-lambda": "^8.10.108",
		"@types/node": "^14.0.0",
		"aws-lambda": "^1.0.7",
		"eslint": "^8.26.0",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
